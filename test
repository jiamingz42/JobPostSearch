         <select id="TldSwitcher" name="tldSwitcher">
         </select>
         <script data-desc="tldSwitcher.tag">
          /* <![CDATA[ */
    var tld = window.location.href,
        $select = $('#TldSwitcher'),
        tldId = 1;

    $.ajax({
        url: 'countryPickerAjaxAction.htm',
        dataType: 'json',
        success:function(data){
            for(var i = 0; i < data.length; i++){
                if(data[i].hasOwnProperty('country') && data[i].hasOwnProperty('fullDomainName')){
                    $select.append('<option value="'+data[i].fullDomainName+'" '+(data[i]["id"] === tldId ? "selected" : "")+'>'+data[i].country.defaultName+'</option>')
                }
            }
            $select.gdSelect();
            tld = $select.val();
            $select.on('change', function(){
               if($select.val() != tld){
                   window.location.href = window.location.protocol + '//' + $select.val() + window.location.pathname;
               }
            });
        }
    });

	/* ]]> */
         </script>
